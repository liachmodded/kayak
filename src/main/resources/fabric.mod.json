{
  "schemaVersion": 1,
  "id": "kayak",
  "version": "${version}",

  "name": "kayak",
  "description": "${description}",
  "authors": [
    "liach"
  ],
  "contact": {
    "homepage": "https://${repoOwner}.github.io/${repoName}",
    "sources": "https://github.com/${repoOwner}/${repoName}",
    "issues": "https://github.com/${repoOwner}/${repoName}/issues"
  },

  "license": "MPL-2.0",
  "icon": "assets/${id}/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "com.github.liachmodded.kayak.client.KayakClient::INSTANCE"
    ],
    "main": [
      "com.github.liachmodded.kayak.Kayak::INSTANCE"
    ]
  },
  "mixins": [
    "kayak.mixins.json"
  ],

  "depends": {
    "fabricloader": "${loaderVersionLimit}",
    "minecraft": "${minecraftVersionLimit}"
  }
}
